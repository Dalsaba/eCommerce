<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<header>
		<nav>
			<ul>
				<a href="{{path('app_default')}}">Accueil</a>
				<a href="{{path('app_produit')}}">Produit</a>
				<a href="{{path('app_register')}}">Register</a>
				<a href="{{path('app_login')}}">Connexion</a>
				<a href="{{path('app_logout')}}">Deconnexion</a>
			</ul>
		</nav>
	</header>
	<body>
		<main class="container">
			{% for type, messages in app.flashes %}
				{% for m in messages %}
					<p class="alert alert-{{type}}">{{m}}</p>
				{% endfor %}
			{% endfor %}

			{% block body %}{% endblock %}
		</main>
	</body>
</html>
